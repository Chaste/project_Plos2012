@Article{Osborne2010,
  author = 	{Osborne, J. M.
		and Walter, A.
		and Kershaw, S. K.
		and Mirams, Gary R.
		and Fletcher, A. G.
		and Pathmanathan, Pras
		and Gavaghan, D. J.
		and Jensen, O. E.
		and Maini, P. K.
		and Byrne, H. M.},
  title = 	{A hybrid approach to multi-scale modelling of cancer},
  journal = 	{Philosophical Transactions of the Royal Society A: Mathematical, Physical and Engineering Sciences},
  year = 	{2010},
  month = 	{Nov},
  day = 	{13},
  volume = 	{368},
  number = 	{1930},
  pages = 	{5013--5028},
  abstract = 	{In this paper, we review multi-scale models of solid tumour growth and discuss a middleout framework that tracks individual cells. By focusing on the cellular dynamics of a healthy colorectal crypt and its invasion by mutant, cancerous cells, we compare a cellcentre, a cell-vertex and a continuum model of cell proliferation and movement. All models reproduce the basic features of a healthy crypt: cells proliferate near the crypt base, they migrate upwards and are sloughed off near the top. The models are used to establish conditions under which mutant cells are able to colonize the crypt either by top-down or by bottom-up invasion. While the continuum model is quicker and easier to implement, it can be difficult to relate system parameters to measurable biophysical quantities. Conversely, the greater detail inherent in the multi-scale models means that experimentally derived parameters can be incorporated and, therefore, these models offer greater scope for understanding normal and diseased crypts, for testing and identifying new therapeutic targets and for predicting their impacts. {\textcopyright} 2010 The Royal Society},
  issn = 	{1364503X},
  doi = 	{10.1098/rsta.2010.0173},
  url = 	{http://dx.doi.org/10.1098/rsta.2010.0173},
}


@Article{Pathmanathan2009,
  author = 	{Pathmanathan, Pras
		and Cooper, Jonathan
		and Fletcher, A. G.
		and Mirams, Gary R.
		and Murray, Philip J.
		and Osborne, J.
		and Pitt-Francis, J.
		and Walter, A.
		and Chapman, S. J.},
  title = 	{A computational study of discrete mechanical tissue models},
  journal = 	{Physical Biology},
  year = 	{2009},
  month = 	{Sep},
  volume = 	{6},
  number = 	{3},
  pages = 	{036001},
  abstract = 	{A computational study of discrete 'cell-centre' approaches to modelling the evolution of a collection of cells is undertaken. The study focuses on the mechanical aspects of the tissue, in order to separate the passive mechanical response of the model from active effects such as cell-growth and cell division. Issues which arise when implementing these models are described, and a series of numerical mechanical experiments is performed. It is shown that discrete tissues modelled this way typically exhibit elastic-plastic behaviour under slow compression, and act as a brittle linear elastic solid under slow tension. Both overlapping spheres and Voronoi-tessellation-based models are examined, and the effect of different cell-cell interaction force laws on the bulk mechanical properties of the tissue is determined. This correspondence allows parameters in the cell-based model to be chosen to be compatible with bulk tissue measurements},
  issn = 	{1478396714783975},
  url = 	{http://iopscience.iop.org/1478-3975/6/3/036001/pdf/1478-3975_6_3_036001.pdf},
}



